{
  "name": "@tutods/components",
  "version": "1.0.0",
  "description": "React generic components that can be used in any scenario",
  "author": {
    "name": "Daniel Sousa @TutoDS",
    "url": "https://github.com/tutods"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:tutods/lib.git",
    "directory": "packages/components"
  },
  "homepage": "https://github.com/tutods/lib/blob/main/packages/components/README.md",
  "license": "MIT",
  "type": "module",
  "keywords": ["react", "components", "typescript", "jsx", "tsx"],
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  }
}
