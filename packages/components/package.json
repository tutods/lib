{
  "name": "@tutods/components",
  "version": "0.3.1",
  "description": "React generic components that can be used in any scenario",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Daniel Sousa @TutoDS",
    "url": "https://github.com/tutods"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:tutods/lib.git",
    "directory": "packages/components"
  },
  "homepage": "https://github.com/tutods/lib/blob/main/packages/components/README.md",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "components",
    "typescript",
    "jsx",
    "tsx"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@tutods/typescript-config": "0.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  }
}
